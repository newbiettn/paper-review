@inproceedings{ ISI:000280543604135,
Author = {Georga, Eleni and Protopappas, Vasilios and Guillen, Alejandra and Fico,
   Giuseppe and Ardigo, Diego and Teresa Arredondo, Maria and Exarchos,
   Themis P. and Polyzos, Demosthenes and Fotiadis, Dimitrios I.},
Book-Group-Author = {{IEEE}},
Title = {{Data Mining for Blood Glucose Prediction and Knowledge Discovery in
   Diabetic Patients: The METABO Diabetes Modeling and Management System}},
Booktitle = {{2009 ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN MEDICINE
   AND BIOLOGY SOCIETY, VOLS 1-20}},
