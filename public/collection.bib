@inproceedings{ ISI:000178928400099,
Author = {Hayes, R and Grimson, B and Pacini, G and Nolan, JJ},
Editor = {{Surjan, G and Engelbrecht, R and McNair, P}},
Title = {{A strategy to enhance the usability of the minimal model as used with
   the intravenous glucose tolerance test in diabetes research and clinical
   management}},
Booktitle = {{HEALTH DATA IN THE INFORMATION SOCIETY}},
