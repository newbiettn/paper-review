@inproceedings{ ISI:000262404502145,
Author = {Tsipouras, Markos G. and Exarchos, Themis P. and Fotiadis, Dimitrios I.},
Book-Group-Author = {{IEEE}},
Title = {{Automated creation of transparent fuzzy models based on decision trees -
   application to diabetes diagnosis}},
Booktitle = {{2008 30th Annual International Conference of the IEEE Engineering in
   Medicine and Biology Society, Vols 1-8}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
Year = {{2008}},
Pages = {{3799-3802}},
Note = {{30th Annual International Conference of the
   IEEE-Engineering-in-Medicine-and-Biology-Society, Vancouver, CANADA, AUG
   20-24, 2008}},
Organization = {{DEVICIX; Green Coll; Natl Inst Hlth; NIBIB; NSF; PLEXON Inc; UBC Engn
   Biomed Engn; Univ Washington, Coll Engn; Bentham Sci Publ Ltd; Recent
   Patents Biomed Engn; Recent Patents Engn}},
Abstract = {{In this paper we propose a novel approach for the simplification of a
   fuzzy model. Initially, we employ a methodology for the automated
   generation of fuzzy models based on decision trees. The methodology is
   realized in three stages. Initially, a crisp model is created from a
   decision tree, induced from the data. Then, the crisp model it is
   transformed to a fuzzy one. Finally, in the third stage, all parameters
   entering the fuzzy model are optimized. The simplification technique is
   based on the pruning of the initial decision tree. The proposed approach
   is applied for diabetes diagnosis and the obtained results indicate its
   efficiency and effectiveness.}},
