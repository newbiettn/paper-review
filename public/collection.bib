@inproceedings{ ISI:000260855900114,
Author = {Wang, H. C. and Yang, P. C. and Hu, L. C.},
Editor = {{Kim, SI and Suh, TS}},
Title = {{Learner Behavior Prediction for Diabetes Health Education Based on
   Sequence Learning Algorithm}},
Booktitle = {{WORLD CONGRESS ON MEDICAL PHYSICS AND BIOMEDICAL ENGINEERING 2006, VOL
   14, PTS 1-6}},
