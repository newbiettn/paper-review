@inproceedings{ ISI:000247284703183,
Author = {Palerm, Cesar C. and Rodriguez-Fernandez, Maria and Bevier, Wendy C. and
   Zisser, Howard and Banga, Julio R. and Jovanovic, Lois and Doyle, III,
   Francis J.},
Book-Group-Author = {{IEEE}},
Title = {{Robust parameter estimation in a model for glucose kinetics in type 1
   diabetes subjects}},
