@inproceedings{ ISI:000244105600023,
Author = {Eapen, Valsamma and Mabrouk, Abdel Azim and Sabri, Sufian and
   Bin-Othman, Salem},
Editor = {{Adeghate, E and Saadi, H and Adem, A and Obineche, E}},
Title = {{A controlled study of psychosocial factors in young people with diabetes
   in the United Arab Emirates}},
Booktitle = {{DIABETES MELLITUS AND ITS COMPLICATIONS: MOLECULAR MECHANISMS,
   EPIDEMIOLOGY, AND CLINICAL MEDICINE}},
Series = {{ANNALS OF THE NEW YORK ACADEMY OF SCIENCES}},
Year = {{2006}},
Volume = {{1084}},
Pages = {{325-328}},
Note = {{International Conference on Recent Advances in Diabetes Mellitus and Its
   Complications, Al Ain, U ARAB EMIRATES, MAR 06-09, 2006}},
Organization = {{UAE Univ, Fac Med \& Hlth Sci; Dept Hlth \& Med Serv, Dubai; Etihad
   Airways; Merck; Aventis; Servier; Life Scan, Johnson \& Johnson; Roche
   Diagnost}},
Abstract = {{Psychosocial factors were studied in 30 young people with diabetes and
   30 control subjects in the UAE. Patients perceived themselves more
   negatively than their parents on several domains. Also, they had lower
   scores in the areas of physical appearance and athletic competence when
   compared to control subjects. There were significant gender differences,
   with girls perceiving themselves more negatively. Better maternal
   education and availability of social support were associated with a
   positive self-image in the child. Parents of children exhibiting
   behavioral/emotional problems had a higher score on the parental General
   Health Questionnaire. Awareness and understanding of these psychosocial
   variables can help healthcare providers to target these issues as part
   of comprehensive diabetes management.}},
