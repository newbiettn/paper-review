ISBN = {{0-7803-7952-7}},
Unique-ID = {{ISI:000186578600271}},
}

@inproceedings{ ISI:000222895200036,
Author = {Iokibe, T and Yoneda, M and Kakita, K},
Book-Group-Author = {{IEEE}},
Title = {{Chaos based blood glucose prediction and insulin adjustment for diabetes
   mellitus}},
Booktitle = {{IEEE EMBS APBME 2003}},
Year = {{2003}},
Pages = {{86-87}},
Note = {{IEEE EMBS Asian-Pacific Conference on Biomedical Engineering
   (APBME2003), Keihanna, JAPAN, OCT 20-22, 2003}},
Organization = {{IEEE EMBS}},
Abstract = {{Many diabetes mellitus (DM) patients are concerned about unstable blood
   glucose, even with regular monitoring by a family doctor. The insulin
   preparation shows peak action at several hours after subcutaneous
   administration. For this reason, unstable blood glucose is often caused
   by intensive insulin therapy with self-monitored blood glucose (SMBG).
   The insulin requirement is determined in proportion to fasting blood
   glucose (FBG) in the sliding scale method. A fixed amount of insulin is
   administered regardless of FBG level in the constant insulin method.
   It is indispensable to estimate FBG at peak time, A hen insulin works
   the hardest, to obtain the appropriate effect of insulin administration.
   We employed the local fuzzy reconstruction method based on chaos theory
   for predicting FBG at peak time. The direction to change the FBG at peak
   time is predicted with a 70-90\% success rate. The amount of insulin
   administration is adjusted based on predicted FBG level. After
   predictive glycemic control (PGC) for around one year, the FBG average
   approached the normal range, standard deviation (SD) reduced by half,
   and hyperglycemia decreased.}},
DOI = {{10.1109/APBME.2003.1302596}},
ISBN = {{0-7803-7943-8}},
Unique-ID = {{ISI:000222895200036}},
}

@inproceedings{ ISI:000187496100021,
