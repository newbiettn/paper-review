Unique-ID = {{ISI:000280543604135}},
}

@inproceedings{ ISI:000280543605102,
Author = {Seto, Emily and Istepanian, Robert S. H. and Cafazzo, Joseph A. and
   Logan, Alexander and Sungoor, Ala},
